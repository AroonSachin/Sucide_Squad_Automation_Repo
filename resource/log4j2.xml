<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="abc" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="commonuseractions" level="trace" additivity="false">
			<AppenderRef ref="abc" />
		</Logger>
		<Root level="trace">
			<AppenderRef ref="abc" />
		</Root>
	</Loggers>
</Configuration>